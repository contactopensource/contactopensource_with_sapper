{
  "name": "contactopensource",
  "description": "Contact Open Source",
  "version": "0.1.0",
  "license": " GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git://github.com/contactopensource/contactopensource_with_sapper.git"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "test-realworld": "(npm run build && export PORT=3100 && node __sapper__/build) & cypress run",
    "deploy": "make deploy"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "express-session": "^1.16.2",
    "marked": "^0.7.0",
    "node-fetch": "^2.6.1",
    "polka": "^0.5.2",
    "session-file-store": "^1.2.0",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "chokidar": "^3.0.1",
    "cypress": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.16",
    "svelte": "^3.12.1",
    "svelte-loader": "^2.13.4"
  },
  "now": {
    "alias": "svelte-realworld.now.sh",
    "files": [
      "__sapper__",
      "yarn.lock"
    ]
  }
}
